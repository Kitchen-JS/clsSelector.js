{
  "name": "clsselector",
  "description": "A JavaScript Selector that handles json data",
  "version": "v3.0.4",
  "scripts": {
    "build": "tailwindcss -i ./tailwindInput/clsSelector.css -o ./www/content/css/clsSelector.css && node build.js",
    "build-run": "tailwindcss -i ./tailwindInput/clsSelector.css -o ./www/content/css/clsSelector.css --watch",
    "test:chrome": "npx playwright test --headed --browser=chromium",
    "test:firefox": "npx playwright test --headed --browser=firefox",
    "test:safari": "npx playwright test --headed --browser=webkit"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.10",
    "express": "^4.18.2",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.2"
  }
}
